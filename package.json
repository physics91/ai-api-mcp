{
  "name": "@physics91org/ai-api-mcp",
  "version": "0.2.0",
  "description": "MCP server for integrating multiple AI APIs",
  "type": "module",
  "bin": {
    "ai-api-mcp": "run.js"
  },
  "scripts": {
    "start": "node run.js",
    "docker:build": "docker build -t ai-api-mcp .",
    "docker:run": "docker run -it --rm --env-file .env ai-api-mcp",
    "docker:compose": "docker-compose up"
  },
  "keywords": [
    "mcp",
    "ai",
    "openai",
    "anthropic",
    "gemini",
    "grok"
  ],
  "author": "",
  "license": "MIT",
  "engines": {
    "node": ">=16.0.0"
  },
  "dependencies": {
    "cross-spawn": "^7.0.6"
  },
  "main": "run.js",
  "directories": {
    "doc": "docs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/physics91/ai-api-mcp.git"
  },
  "bugs": {
    "url": "https://github.com/physics91/ai-api-mcp/issues"
  },
  "homepage": "https://github.com/physics91/ai-api-mcp#readme",
"publishConfig":{
"access":"public"
}
}
